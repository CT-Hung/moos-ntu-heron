//uTimerScript script for group 1 round 3

ProcessConfig = uTimerScript                                    
{                                                               
  AppTick   = 4                                                 
  CommsTick = 4                                                 
                                                                
 // Logic condition that must be met for script to be unpaused 
 condition = AQUATICUS_GAME_ALL=play                            
 // Event(s) are the key components of the script

//establish group #
  event = var=GROUP, val=1, time = 0
//establish round #
  event = var = ROUND, val=3, time = 0

//Trial 1: E5
  event          = var=SCENARIO, val="GAME3A", time=0
  event          = var=ACTION_RED_ONE, val="ATTACK_RIGHT", time=0
  event          = var=ACTION_RED_TWO, val="ATTACK", time=0


//Trial 2:  H2
  event          = var=SCENARIO, val="GAME3B", time=480
  event          = var=ACTION_RED_ONE, val="ATTACK_LEFT", time=480
  event          = var=ACTION_RED_TWO, val="DEFEND", time=480


//Trial 3: E1
  event          = var=SCENARIO, val="GAME3C", time=960
  event          = var=ACTION_RED_TWO, val="ATTACK_RIGHT", time=960
  event          = var=ACTION_RED_ONE, val="PROTECT", time=960
  event          = var=AGGRESSIVE_RED_ONE, val="false", time=960


//Trial 4: H1 fail
  event          = var=SCENARIO, val="GAME3D", time=1440
  event          = var=ACTION_RED_ONE, val="ATTACK_LEFT", time=1440
  event          = var=ACTION_RED_TWO, val="STATION_MIDLEFT", time=1440

}