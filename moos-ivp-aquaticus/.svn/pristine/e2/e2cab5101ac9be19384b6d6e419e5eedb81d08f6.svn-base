//uTimerScript script for group 1 round 1

ProcessConfig = uTimerScript                                    
{                                                               
  AppTick   = 4                                                 
  CommsTick = 4                                                 
                                                                
 // Logic condition that must be met for script to be unpaused 
 condition = AQUATICUS_GAME_ALL=play                            
 // Event(s) are the key components of the script

//establish group #
  event = var=GROUP, val=1, time = 0
//establish round #
  event = var = ROUND, val=1, time = 0
//establish High or Low autonomy
  event = var = SELF_AUTHORIZE_BLUE_TWO, val="TRUE"
//establish that out autonomous teammate is realiable at the start
  event = var=RELIABLE_BLUE_TWO, val="TRUE"

//Trial 1: H1
  event          = var=SCENARIO, val="GAME1A", time=0
  event          = var=ACTION_RED_ONE, val="ATTACK_LEFT", time=0
  event          = var=ACTION_RED_TWO, val="ATTACK", time=0

//Trial 2: E2
  event          = var=SCENARIO, val="GAME1B", time=480
  event          = var=ACTION_RED_ONE, val="ATTACK", time=480
  event          = var=ACTION_RED_TWO, val="PROTECT", time=480
  event          = var=AGGRESSIVE_RED_TWO, val="false", time=480

//Trial 3: E3
  event          = var=SCENARIO, val="GAME1C", time=960
  event          = var=ACTION_RED_ONE, val="STATION_MIDFIELD", time=960
  event          = var=ACTION_RED_TWO, val="ATTACK", time=960

//Trial 4: H4 fail
  event          = var=SCENARIO, val="GAME1D", time=1440
  event          = var=ACTION_RED_ONE, val="STATION_MIDLEFT", time=1440
  event          = var=ACTION_RED_TWO, val="ATTACK", time=1440

}